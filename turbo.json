{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "destroy": {
      "cache": false
    },
    "db:generate": {
      "cache": false,
      "dependsOn": [
        "check"
      ]
    },
    "check": {
      "cache": false
    },
    "check:fix": {
      "cache": false
    }
  }
}