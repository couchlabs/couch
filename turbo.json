{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "test": {
      "cache": false
    },
    "fix": {
      "cache": false
    },
    "db:generate": {
      "cache": false,
      "dependsOn": [
        "test"
      ]
    },
    "infra:deploy": {
      "cache": false
    },
    "infra:destroy": {
      "cache": false
    }
  }
}